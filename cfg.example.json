{
    "debug": true,
	"debugmetric":{
		"endpoints":["192.168.56.101","192.168.56.102"],
		"metrics":["switch.if.In","switch.if.Out"],
		"tags":"ifName=Fa0/1"
	},
	"switch":{
	   "enabled": true,
		"ipRange":[
            "192.168.56.101/32",      
            "192.168.56.102/32",
            "172.16.114.233" 
 		],
		"gosnmp":true,
 		"pingTimeout":300,
		"pingRetry":4,
		"community":"public",
		"snmpTimeout":1000,
		"snmpRetry":5,
		"ignoreIface": ["Nu","NU","Vlan","Vl"],
		"ignorePkt": true,
		"ignoreBroadcastPkt": true,
		"ignoreMulticastPkt": true,
		"ignoreDiscards": true,
		"ignoreErrors": true,
		"ignoreOperStatus": true,
		"ignoreUnknownProtos":true,
		"ignoreOutQLen":true,
		"displayByBit": true,
		"fastPingMode": false,
		"limitConcur": 1000
	}, 
    "mapfile": "hostnames",
    "transfer": {
        "enabled": true,
        "addr": "127.0.0.1:8433",
        "interval": 60,
        "timeout": 1000
    },
    "http": {
        "enabled": true,
        "listen": ":1989"
    }
}
